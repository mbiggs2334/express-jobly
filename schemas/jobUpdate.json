    "default": {},
    "examples": [
        {
            "title": "Developer",
            "salary": 50000,
            "equity": "1.0",
            "companyHandle": "c2"
        }
    ],
    "required": [
    ],
    "properties": {
        "title": {
            "$id": "#/properties/title",
            "type": "string"
        },
        "salary": {
            "$id": "#/properties/salary",
            "type": "integer"
        },
        "equity": {
            "$id": "#/properties/equity",
            "type": "number"
        },
        "companyHandle": {
            "$id": "#/properties/companyHandle",
            "type": "string"
        }
    },
    "additionalProperties": true
